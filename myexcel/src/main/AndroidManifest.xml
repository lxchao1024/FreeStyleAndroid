<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lxchao.myexcel">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.digit.OpinionActivity"
            android:label="判断" />
        <activity
            android:name=".ui.simulation.SimulationOpinionActivity"
            android:label="判断" />
        <activity
            android:name=".ui.digit.QaActivity"
            android:label="问答" />
        <activity
            android:name=".ui.digit.CheckedActivity"
            android:label="多选" />
        <activity
            android:name=".ui.simulation.SimulationCheckedActivity"
            android:label="多选" />
        <activity
            android:name=".ui.digit.SingleActivity"
            android:label="单选" />
        <activity
            android:name=".ui.simulation.SimulationSingleActivity"
            android:label="单选" />
        <activity
            android:name=".ui.digit.BlankActivity"
            android:label="填空" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>